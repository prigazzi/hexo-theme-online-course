<% if (config.disqus_shortname){ %>
<script>
  var disqus_shortname = '<%= config.disqus_shortname %>';
  <% if (page.permalink){ %>
  var disqus_url = '<%= page.permalink %>';
  <% } %>
  (function(){
    var dsq = document.createElement('script');
    dsq.type = 'text/javascript';
    dsq.async = true;
    dsq.src = '//go.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
  })();
</script>
<% } %>

<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
<script src="<%- config.root %>js/equal-rowheight.min.js" type="text/javascript"></script>
<% if (theme.fancybox){ %>
<link rel="stylesheet" href="<%- config.root %>fancybox/jquery.fancybox.css" media="screen" type="text/css">
<script src="<%- config.root %>fancybox/jquery.fancybox.pack.js"></script>
<% } %>
<script>
  $(function()
  {
        var querystring = deparam(location.search);
        if (typeof querystring.lastLesson != "undefined")
    {
      var link = $("#lastLesson").css("display", "block");
    }

        var sharing = $(".sharing");
        sharing.attr("href", sharing.attr("href") + location.href);
        if (courses[querystring.course]) {
          document.title = "Finished " + courses[querystring.course].title;
        }
  });
</script>
<%- js('js/script') %>